&soc {
	tlmm_pinmux: pinctrl@1000000 {
		compatible = "qcom,msm-tlmm-8916";
		reg = <0x1000000 0x300000>;
		interrupts = <0 208 0>;

		/*****************************************/
		/*  pinctrl customize setting            */
		/*****************************************/
		dtv_rst_n {
			/* GPIO_09 */
			qcom,pins = <&gp 9>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "dtv_rst_n";

			dtv_rst_active: active {
				drive-strength = <2>;
				bias-disable;
			};

			dtv_rst_suspend: suspend {
				drive-strength = <2>;
				bias-disable;
			};
		};

		dtvint_n {
			/* GPIO_99 */
			qcom,pins = <&gp 99>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "dtvint_n";

			dtvint_active: active {
				drive-strength = <2>;
				bias-disable;
			};

			dtvint_suspend: suspend {
				drive-strength = <2>;
				bias-disable;
			};
		};

	};
};

